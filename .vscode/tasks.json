{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/OrdersCollector.sln"
            ],
            "group": "build",
            "problemMatcher": "$msCompile"
        },
        {
            "label": "test",
            "command": "dotnet",
            "type": "process",
            "args": [
                "test",
                "${workspaceFolder}/OrdersCollector.Tests/OrdersCollector.Tests.csproj"
            ],
            "group": "test",
            "problemMatcher": "$msCompile"
        }
    ]
}